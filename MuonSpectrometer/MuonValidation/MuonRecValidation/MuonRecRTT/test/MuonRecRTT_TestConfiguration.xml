          <keepFilePattern>*.ps</keepFilePattern>
          <keepFilePattern>*.gif</keepFilePattern>
	  <keepFilePattern>*.png</keepFilePattern>
          <keepFilePattern>*.log</keepFilePattern>
          <keepFilePattern>*.C</keepFilePattern>
          <keepFilePattern>*.html </keepFilePattern>
          <auxFilePattern>CpuMem_Plots.C</auxFilePattern>
          <auxFilePattern>TrkD3PDsPlotsMaker.C</auxFilePattern>
          <auxFilePattern>ALL_MuonRecRTT_*.html</auxFilePattern>
          <action position="a">
            <modulename>RttLibraryTools</modulename>
            <testname>FileGrepper</testname>
            <arg>
              <argname>inputFile</argname>
              <argvalue>*_log</argvalue>
            </arg>
            <arg>
              <argname>outputFile</argname>
              <argvalue>logExtract.log</argvalue>
            </arg>
            <arg>
              <argname>searchList</argname>
              <argvalue>leaving with code 0</argvalue>
            </arg>
            <arg>
              <argname>vetoList</argname>
              <argvalue>FATAL,segmentation violation,bus error,Floating point exception</argvalue>
            </arg>
          </action>
          <action position="b">
            <modulename>RttLibraryTools</modulename>
            <testname>FileGrepper</testname>
            <arg>
              <argname>inputFile</argname>
              <argvalue>*_log</argvalue>
            </arg>
            <arg>
              <argname>outputFile</argname>
              <argvalue>libraries.txt</argvalue>
            </arg>
            <arg>
              <argname>searchList</argname>
              <argvalue>AthDsoLogger</argvalue>
            </arg>
          </action>
	  <action position="c">
             <modulename>RttLibraryTools</modulename>
              <testname>ROOTMacroRunner</testname>
              <arg>
                 <argname>macro</argname>
                 <argvalue>TrkD3PDsPlotsMaker.C</argvalue>
              </arg>
          </action>
          <action position="d">
               <modulename>MuonRecRTT_dynamic_pagemaker</modulename>
               <testname>MuonRecRTT_dynamic_pagemaker</testname>
          </action>
      </jobGroup> 

