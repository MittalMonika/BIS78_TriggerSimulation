namespace Trk {

inline Trk::MultiComponentState::MultiComponentState()
  : std::vector<Trk::ComponentParameters>()
{
  this->reserve(72);
}

inline Trk::MultiComponentState::MultiComponentState(const Trk::ComponentParameters& componentParameters)
  : std::vector<Trk::ComponentParameters>()
{
  this->reserve(72);
  this->push_back(componentParameters);
}

};
