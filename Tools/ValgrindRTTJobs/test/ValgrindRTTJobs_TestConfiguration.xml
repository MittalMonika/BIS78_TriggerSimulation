    <jobTransform userJobId="HelloValgrind">
      <doc>Short Testjob</doc>
      <jobTransformJobName>HelloValgrind</jobTransformJobName>
      <jobTransformCmd>
	valgrind_trf.py VGHelloWorldOptions.py stacktraceDepth=50
      </jobTransformCmd>
      <group>Valgrind_JobTransforms</group>
      <queue>long</queue>
    </jobTransform>


    
    <jobTransform userJobId="GenerateEvents">
      <doc>Short MC Job</doc>
      <jobTransformJobName>GenerateEvents</jobTransformJobName>
      <jobTransformCmd>
	valgrind_trf.py McGenerationPythia.py stacktraceDepth=25
      </jobTransformCmd>
      <group>Valgrind_JobTransforms</group>
      <queue>long</queue>

      <testToRemove>
	<jobGroupName>Top</jobGroupName>
	<testidentifier>CheckFileRunner0</testidentifier>
      </testToRemove>
      
    </jobTransform>


    
    <jobTransform userJobId="collisionsRAWtoESD">
      <doc>collisions RAW to ESD collisions all</doc>
      <jobTransformJobName>collisionsRAWtoESD</jobTransformJobName>
      <jobTransformCmd>
	valgrind_trf.py recoTrf=Reco_trf.py+inputBSFile=/afs/cern.ch/atlas/project/rig/referencefiles/dataStreams_10Events/data11_7TeV.00178044.physics_JetTauEtmiss.merge.RAW._lb0092._SFO-4._0001.1_25events+outputESDFile=myESD.pool.root+maxEvents=2+autoConfiguration=everything+preExec=rec.doPerfMon=False,,rec.doDetailedPerfMon,,rec.doSemiDetailedPerfMon=False,,'"rec.UserAlgs=[\"ValgrindRTTJobs/vgOpts.py\"]"'+postExec=topSequence.TimeOut=0+--ignoreunknown stacktraceDepth=50
      </jobTransformCmd>
      <group>Valgrind_JobTransforms</group>
      <queue>long</queue>
    </jobTransform>


    
    <jobTransform userJobId="ESD2ESDnoreco">
      <doc>An ESD to ESD job</doc>
      <jobTransformJobName>ESDtoESD</jobTransformJobName>
      <jobTransformCmd>
	valgrind_trf.py RecExCommon/esdtoesd.py needInputFile=ESD stacktraceDepth=25 athenaOpt=rec.UserAlgs="['ValgrindRTTJobs/vgOpts.py']"
      </jobTransformCmd>
      <group>Valgrind_JobTransforms</group>
      <queue>long</queue>
    </jobTransform>


    
    <jobTransform userJobId="ESD2ESDwithreco">
      <doc>An ESD to ESD job with reco</doc>
      <jobTransformJobName>ESDtoESDwRec</jobTransformJobName>
      <jobTransformCmd>
	valgrind_trf.py recoTrf=Reco_trf.py+inputESDFile=ESD.pool.root+outputESDFile=newESD.pool.root+preExec='"rec.doESD.unlock(),,rec.doESD.set_Value_and_Lock(True),,rec.doPerfMon=False,,rec.doDetailedPerfMon=False,,rec.doSemiDetailedPerfMon=False,,rec.doTrigger=False,,rec.UserAlgs=[\"ValgrindRTTJobs/vgOpts.py\"]"'+autoConfiguration=everything+postExec=topSequence.TimeOut=0+preInclude=ValgrindRTTJobs/noWriteESD.py needInputFile=ESD stacktraceDepth=15
